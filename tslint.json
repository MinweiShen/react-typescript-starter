{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-shorthand": true,
    "trailing-comma": [
      true,
      {
        "multiline": "ignore",
        "singleline": "never"
      }
    ],
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "quotemark": [true, "single", "jsx-double"]
  },
  "rulesDirectory": []
}